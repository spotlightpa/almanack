{
  "name": "almanack",
  "version": "0.1.0",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "yarn run test:eslint && yarn run test:prettier",
    "test:eslint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "test:prettier": "prettier --ignore-path .gitignore --check './*.{js,vue,yml,json}' 'src/**/*.{js,vue,yml,json}'",
    "lint": "yarn run lint:eslint && yarn run lint:prettier",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "lint:prettier": "prettier --ignore-path .gitignore --write './*.{js,vue,yml,json}' 'src/**/*.{js,vue,yml,json}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@sentry/vue": "^7.9.0",
    "@vueuse/head": "^1.0.19",
    "bulma": "^0.9.4",
    "core-js": "^3.24.1",
    "journalize": "^2.5.1",
    "netlify-identity-widget": "^1.9.2",
    "vue": "^3.2.37",
    "vue-router": "^4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.12.16",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^3.0.1",
    "sass": "^1.54.3",
    "unplugin-vue-components": "0.25.1",
    "vite": "^4.0.1",
    "vite-plugin-eslint": "^1.7.0"
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
